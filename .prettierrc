{
  "overrides": [
    {
      "files": "*.json",
      "options": {
        "trailingComma": "none",
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true
      }
    },
    {
      "files": "*.java",
      "options": {
        "tabWidth": 4,
        "semi": true,
        "singleQuote": true,
        "printWidth": 200
      }
    }
  ]
}
